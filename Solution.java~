
import java.util.Scanner;

public class Solution {
  
  public static void main(String[]args)
  {
    Scanner input = new Scanner(System.in);
    
    String value;
    
    value = input.nextLine();
    
    input.close();
    
    System.out.println("Hello, World.");
    System.out.println(value);
  }
    
  
}
